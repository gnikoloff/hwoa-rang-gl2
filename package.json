    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "uid": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnikoloff/hwoa-rang-gl2.git"
  },
  "bugs": {
    "url": "https://github.com/gnikoloff/hwoa-rang-gl2/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Georgi Nikolov",
    "email": "connect@georgi-nikolov.com",
