{
  "name": "hwoa-rang-gl2",
  "version": "0.0.1",
  "description": "minimal WebGL2.0 library",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "start:dev": "tsup ./src --sourcemap --watch --format esm,cjs --dts",
    "start:build": "tsup ./src --minify --format esm,cjs --dts"
